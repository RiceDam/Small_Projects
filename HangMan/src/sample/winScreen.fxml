<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.winController">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label layoutX="228.0" layoutY="28.0" text="You Win!">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <Button fx:id="quitBut" layoutX="333.0" layoutY="231.0" mnemonicParsing="false" onAction="#quit" text="Quit" />
            <Button fx:id="playBut" layoutX="228.0" layoutY="231.0" mnemonicParsing="false" onAction="#playAgain" text="Play Again" />
            <Label fx:id="word" layoutX="228.0" layoutY="128.0">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</AnchorPane>
